mixin socialIcons(networks)
  each networkUrl, network in networks
    case network
      when "facebook"
        a.social_icon--facebook(href="#{ networkUrl }", target="_blank")&attributes(attributes)
      when "twitter"
        a.social_icon--twitter(href="#{ networkUrl }", target="_blank")&attributes(attributes)
      when "google-plus"
        a.social_icon--google-plus(href="#{ networkUrl }", target="_blank")&attributes(attributes)
      when "skype"
        a.social_icon--skype(href="#{ networkUrl }", target="_blank")&attributes(attributes)
        
mixin icon(icon)
  case icon
    when "location"
      svg.icon--location(xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 56" preserveAspectRatio="xMidYMid meet" width="32" height="56")
        image(width="32" height="56" xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="data:img/png;base64,")
    when "plus"
      svg.session__icon(xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" preserveAspectRatio="xMidYMid" width="23" height="23" viewBox="0 0 23 23")&attributes(attributes)
        image(xlink:href="data:img/png;base64," width="23" height="23")
    when "minus"
      svg.session__icon(xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" preserveAspectRatio="xMidYMid" width="23" height="23" viewBox="0 0 23 23")&attributes(attributes)
        image(xlink:href="data:img/png;base64," width="23" height="23")
    when "coffe"
